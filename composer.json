{
    "name": "studiosystems/odata-client",
    "version": "0.8.0",
    "description": "Studiosystems forked from Saint Systems OData Client for PHP",
    "keywords": [
        "odata",
        "rest",
        "php"
    ],
    "homepage": "https://github.com/studiosystems/odata-client-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "StudioSystems",
            "email": "contact@studiosystems.fr"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^2.0 || ^3.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.5",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "autoload": {
        "files": [
            "src/Core/helpers.php"
        ],
        "psr-4": {
            "Studiosystems\\OData\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
